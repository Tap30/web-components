{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    {
      "repo": "Tap30/web-components"
    }
  ],
  "commit": false,
  "fixed": [],
  "linked": [
    ["@tapsioss/web-components", "@tapsioss/react-components"],
