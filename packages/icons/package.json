{
  "sideEffects": false,
  "name": "@tapsioss/icons",
  "version": "0.2.1",
  "type": "module",
  "homepage": "https://tap30.github.io/web-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tap30/web-components.git",
    "directory": "packages/icons"
  },
  "files": [
    "./dist/**/*.js",
    "./dist/**/*.d.ts",
    "./dist/**/package.json",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "release": "pnpm publish . --tag latest --access public --no-git-checks"
  }
}
